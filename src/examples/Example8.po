import std;

namespace Example
{
    struct test1
    {
        i64 x;
    }

    struct test2
    {
        i64 x;
        i64 y;
    }

    static test2 test(test1 t1, test2 t2, i64 p)
    {
        i64 x = t1.x + t2.x + t2.y + p;
        test2 r;
        r.x = x;
        r.y = 11;
        return r;
    }

    static void main()
    {
        test1 t1;
        test2 t2;

        t1.x = 10;
        t2.x = 20;
        t2.y = 30;
        test2 r = test(t1, t2, 10);

        i64 x = r.x;
        i64 y = r.y;
    }
}
